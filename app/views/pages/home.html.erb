<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

  <%= simple_form_for @match_lists do |f| %>
   <div class="form-inputs in-line">
    <%= f.input :date, placeholder: "Date", label:"Date", required: true %>
    <%= f.input :start_time, placeholder: "Time", label:"Time", required: true %>
    <%= f.input :food_type, collection: ["Mexican", "French", "Japanese", "Italian"], label: "Gender preferences", required: true %>
    <%= f.submit 'GO!' %>

  </div>
<% end %>
