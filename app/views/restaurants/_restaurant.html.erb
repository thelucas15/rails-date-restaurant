

  <div class="col-xs-12 col-sm-4">

    <div class="card">
      <%= link_to "", restaurant_path(restaurant, date: @date, start_time: @start_time), class: 'card-link' %>
      <div class="card-body">
        <div class="card-picture" style="background-image: linear-gradient(rgba(0,0,0,0.2), rgba(0,0,0,0.1)), url('<%= restaurant.photo %>')">
        <div class="counter">
          <img src="http://placehold.it/30x30" alt="" class="card-avatar avatar">
           <%=  restaurant.match_lists.count %>
          </div>

        </div>
        <div class="card-likes row">
        <div class="col-xs-9">
          <h6><%= restaurant.name %></h6>
        </div>
        <div class="col-xs-3">
          <h6><i class="fa fa-user-o"></i>140</h6>
        </div>
        </div>
      </div>

    <!--   <a class="card-link" href="#" ></a> -->

  </div>
</div>




